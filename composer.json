{
    "name": "veeeeeRySeXy/laravel-quiz",
    "type": "library",
    "description": "Library to make easy integration with questions anf quizzes",
    "authors": [
        {
            "name": "Eugene Mosunov",
            "email": "eugene.mosunov@saritasa.com"
        }
    ],
    "autoload": {
        "psr-4" : {
            "Saritasa\\LaravelQuiz\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Saritasa\\LaravelQuiz\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=7.3",
        "ext-json": "*",
        "saritasa/laravel-controllers": "^3.3",
        "squizlabs/php_codesniffer": "^3.5",
        "srlabs/eloquent-sti": "^3.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.3",
        "phpunit/phpunit": "^9.0"
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "prefer-stable": false,
    "minimum-stability": "beta"
}
